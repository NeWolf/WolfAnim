<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ewolf.wolfanimdemo">

    <application
        android:name="com.newolf.animdemo.App"
        android:allowBackup="true"
        android:icon="@mipmap/tj_y"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tj_y"
        android:supportsRtl="true"
        android:theme="@style/Theme.WolfAnimDemo">
        <activity
            android:name="com.newolf.animdemo.DurationActivity"
            android:exported="true" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>